<p id="notice"><%= notice %></p>

<h1>Listing Rounds</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Course</th>
      <th>Competition</th>
      <th>Tee</th>
      <th>Tour part</th>
      <th>Score</th>
      <th>Division</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rounds.each do |round| %>
      <tr>
        <td><%= round.user_id %></td>
        <td><%= round.course_id %></td>
        <td><%= round.competition_id %></td>
        <td><%= round.tee_id %></td>
        <td><%= round.tour_part_id %></td>
        <td><%= round.score %></td>
        <td><%= round.division %></td>
        <td><%= link_to 'Show', round %></td>
        <td><%= link_to 'Edit', edit_round_path(round) %></td>
        <td><%= link_to 'Destroy', round, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Round', new_round_path %>
